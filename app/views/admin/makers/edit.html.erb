<main class="mb-auto mt-5">
  <div class="container">
    <div class="row ge-edit">
      <div class="h5 my-3 text-center theme col-2 offset-md-1">メーカー編集</div>
    </div>
    <div class="row">
        <%= form_with model: @maker, url: admin_maker_path, local:true do |f| %>
          <div class="form-group form">
            <div style="margin-right:50px">メーカー名</div>
            <div style="margin-right:50px"><%= f.text_field :name %></div>
          </div>
          <%= f.submit "変更を保存", class: "btn btn-success" %>
        <% end %>
    </div>
  </div>
</main>